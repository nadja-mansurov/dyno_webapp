{
  "name": "dynophores_webclient",
  "version": "1.2.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build:ngl:copy",
    "start": "npm run lint && ng serve",
    "build": "ng build",
    "test": "npx jest",
    "lint": "eslint '**/*.ts'",
    "e2e": "ng e2e",
    "buildaot": "ng build --configuration production --output-path dist/dynophores_webclient --base-href /dynophores_webclient/",
    "update": "rm -rf node_modules  && rm -rf package-lock.json && npm i && npm audit fix --force",
    "update:all": "ncu -u && npm i",
    "build:ngl:dts": "cd node_modules/ngl && npm run dts",
    "build:ngl:copy": "copyfiles --flat node_modules/ngl/dist/ngl.js src/assets",
    "build:ngl:dts:copy": "npm run dts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.5",
    "@angular/common": "12.2.5",
    "@angular/compiler": "12.2.5",
    "@angular/core": "12.2.5",
    "@angular/forms": "12.2.5",
    "@angular/platform-browser": "12.2.5",
    "@angular/platform-browser-dynamic": "12.2.5",
    "@angular/router": "12.2.5",
    "@ngrx/store": "12.4.0",
    "@ngrx/store-devtools": "12.4.0",
    "@popperjs/core": "2.10.1",
    "bootstrap": "5.1.1",
    "bootstrap-icons": "1.5.0",
    "idb": "^6.1.3",
    "ng2-charts": "^3.0.0-rc.4",
    "ngl": "^2.0.0-dev.28",
    "rxjs": "6.6.7",
    "subsink": "1.0.2",
    "three": "0.132.2",
    "timers": "^0.1.1",
    "tinycolor2": "1.4.2",
    "tslib": "2.3.1",
    "xml2js": "0.4.23",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.5",
    "@angular/cli": "12.2.5",
    "@angular/compiler-cli": "12.2.5",
    "@types/jasmine": "3.9.0",
    "@types/jest": "27.0.1",
    "@types/node": "16.9.1",
    "@types/three": "0.131.0",
    "@types/tinycolor2": "^1.4.3",
    "@types/xml2js": "0.4.9",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "angular-cli-ghpages": "1.0.0-rc.2",
    "codelyzer": "6.0.2",
    "copyfiles": "2.4.1",
    "eslint": "7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.11.3",
    "jasmine-core": "3.9.0",
    "jasmine-spec-reporter": "7.0.0",
    "jest": "27.1.1",
    "jest-preset-angular": "9.0.7",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "protractor": "7.0.0",
    "ts-node": "10.2.1",
    "typescript": "4.2.3"
  }
}
